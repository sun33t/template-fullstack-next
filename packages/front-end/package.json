{
  "name": "@right-the-rules/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "yarn prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "jsxSingleQuote": false,
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "endOfLine": "auto"
  }
}
